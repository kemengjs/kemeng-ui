{
  "name": "@kemengjs/kemeng-ui",
  "version": "0.0.1-beta.2",
  "description": "kemeng-ui 框架",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "scripts": {
    "dev": "gulp --require ../../node_modules/@esbuild-kit/cjs-loader -f ./internal/build/gulpfile.ts --cwd ./ dev",
    "build": "gulp --require ../../node_modules/@esbuild-kit/cjs-loader -f ./internal/build/gulpfile.ts --cwd ./ build"
  },
  "files": [
    "dist",
    "./package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./utils":{
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.cjs"
    },
    "./package.json": "./package.json",
    "./*": {
      "types": "./dist/components/*",
      "import": "./dist/components/*",
      "require": "./dist/components/*"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kemengjs/kemeng-ui.git"
  },
  "keywords": [],
  "type": "module",
  "author": "kemengwang <1148177968@qq.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@types/lodash": "^4.17.0",
    "@types/react-transition-group": "^4.4.10",
    "picocolors": "^1.0.0",
    "ts-morph": "^22.0.0"
  },
  "peerDependencies": {
    "@linaria/atomic": "^6.1.0",
    "@linaria/core": "^6.1.0",
    "@types/react": "^17.0.0 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "react-transition-group": "^4.4.5"
  }
}
